{
    "name": "Ubytování Slavonice",
    "description": "Server pro správu a zobrazení ubytovacích kapacit ve Slavonicích",
    "keywords": ["slavonice", "ubytování", "pokoje", "dovolená", "hotel", "penzion", "privat"],
    "homepage": "http://ubytovani.slavonice.cz/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/czechcamus/ubytovani-slavonice.git"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "yiisoft/yii2-jui": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
